/* page/index/pages/osuccess/osuccess.wxss */
@import "../../../common/index.wxss";
@import "../../../common/lib/weui.wxss";
.order-status-box {
  height: 96px;
  display: flex;
  background-color: #0087FE;
  align-items: center;
  padding: 30rpx 40rpx;
  color: #fff;
}
.order-status-img {
  width: 50px;
  height: 50px;
  border-radius: 25px;
}
.order-status-txt {
  margin-left: 20rpx;
}
.urgent-box {
  text-align: center;
  padding: 10rpx 30rpx;
}
.urgent-nav {
  width: 90%;
  border-radius: 50px;
  margin: auto;
}
.urgent-btn {
  border-radius: 50px;
  font-size: 15px;
  width: 100%;
  background-color: #ff9500;
  margin-bottom: 10rpx;
}
.urgent-tip {
  color: #999;
}
.kind-list {
  margin-bottom: 20rpx;
  background-color: #fff;
}
.contact-box {
  z-index: 1;
  /* border-top: 2px dashed #93ccff; */
  border-bottom: 2px dashed #93ccff;
}
.contact-name {
  margin: 0 0 20rpx;
  color: #363636;
}
.contact-img-phone,.contact-img-address,.select-list-item-icon{
  width: 30rpx;
  height: 30rpx;
  margin: auto 5px auto 0;
}
.contact-img-phone {
  margin: auto 5px auto 10px;
}
.userphone {
  color: #999;
  /* font-size: 16px; */
}
.contact-address {
 display: flex;
}
.contact-desc {
  color: #999;
  flex: 1;
}
.kind-list-item {
  margin-bottom: 20rpx;
  display: flex;
  align-items: flex-start;
  font-size: 13px;
}
.kind-list-item:last-child {
  margin-bottom: 0;
}
.kind-list-hd {
  width: 70px;
}
.kind-list-bd {
  color: #787878;
  flex: 1;
}
.order-relate-img {
  width: 65px;
  height: 65px;
  margin-right: 20rpx;
}
.price-list-item {
  display: flex;
  align-items: center;
  font-size: 12px;
  color: #999;
  vertical-align: middle;
  margin-bottom: 12px;
}
.price-list-item:last-child {
  border-top: 1px solid #eee;
}
.price-img {
  width: 40px;
  height: 40px;
  border-radius: 35px;
}
.price-txt-title {
  font-size: 14px;
  color: #505050;
}
.price-list-hd{
  display: flex;
  align-items: center;
} 
.price-list-ft {
  flex: 1;
  text-align: right;
}
.price-list-bd {
  /* flex: 2; */
  margin: 0 20rpx;
}
.price-list-bd text {
  display: block;
}
.total-price {
  flex: 1;
  text-align: right;
  padding-top: 20rpx;
}
.hl-price {
  font-size: 20px;
  /* font-weight: bold; */
  color: #505050;
  margin: 0 10rpx;
}
.wx-audio-name{
  margin-top: 20rpx;
}

.wx-audio-time{
  padding-top: 30rpx;
}